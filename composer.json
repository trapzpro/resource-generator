{
    "name": "cloudstudio/resource-generator",
    "description": "Resource Generator for Laravel Nova",
    "keywords": [
        "resource",
        "generator",
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Toni Soriano",
            "email": "hello@cloudstudio.es",
            "role": "Developer"
        },
        {
            "name": "Eric Lagarda",
            "email": "eric@infinety.es",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1.0|^8.0",
        "laravel/nova": "*",
        "doctrine/dbal": "^2.8|^3.0"
    },
    "require-dev": {
        "orchestra/testbench": "^3.6|^4.0|^5.0|^6.0|^7.0",
        "phpunit/phpunit": "^9.5.10"
    },
    "autoload": {
        "psr-4": {
            "Cloudstudio\\ResourceGenerator\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cloudstudio\\ResourceGenerator\\ResourceGeneratorServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}